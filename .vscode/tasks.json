{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Maven: Clean",
            "type": "shell",
            "command": "mvn clean",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$maven"]
        },
        {
            "label": "Maven: Install",
            "type": "shell",
            "command": "mvn install",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$maven"]
        },
        {
            "label": "Maven: Test",
            "type": "shell",
            "command": "mvn test",
            "group": {
                "kind": "test",
                "isDefault": true
            },
            "problemMatcher": ["$maven"]
        },
        {
            "label": "Maven: Package",
            "type": "shell",
            "command": "mvn package",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$maven"]
        }
    ]
}
